#include<iostream>
#include<bits/stdc++.h>
#include<list>
#include<stack>
#include<algorithm>
#include<string.h>
using namespace std;
stack<int>s;
void printNSE(int arr[],int n)
{
	s.push(arr[0]);
	for(int i=1;i<n;i++)
	{
		if(s.empty())
		{
			s.push(arr[i]);
			continue;
		}
		while(s.empty()==false && s.top()>arr[i])
		{
            cout << s.top() << " --> " << arr[i] << endl; 
            s.pop();		
		}
		s.push(arr[i]);
	}
	
	while(s.empty()==false)
	{
		cout<<s.top()<<"-->"<<-1<<endl;
		s.pop();
	}
}

int main() 
{ 
   int arr[] = { 11, 13, 21, 3 }; 
    int n = sizeof(arr) / sizeof(arr[0]); 
    printNSE(arr, n); 
    return 0; 
} 
